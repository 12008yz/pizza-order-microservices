{
  "name": "tariff-aggregator-monorepo",
  "version": "1.0.0",
  "description": "Гигапоиск — проверка доступности интернета по адресу и сравнение тарифов провайдеров",
  "private": true,
  "workspaces": [
    "services/*",
    "frontend"
  ],
  "scripts": {
    "dev": "docker-compose up",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:services": "docker-compose up --scale frontend=0",
    "build": "docker-compose build",
    "build:frontend": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "start:frontend": "cd frontend && npm run start",
    "down": "docker-compose down",
    "logs": "docker-compose logs -f",
    "check-integration": "node scripts/check-services-integration.js",
    "check-api": "node scripts/check-api-gateway.js"
  },
  "keywords": [
    "microservices",
    "nextjs",
    "postgresql",
    "sequelize",
    "tariff",
    "provider",
    "гигапоиск",
    "gigapoisk"
  ],
  "author": "",
  "license": "ISC"
}



